<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Async JavaScript</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <style>
      img{
        width:20px;
        filter: invert(38%) sepia(67%) saturate(6486%) hue-rotate(211deg) brightness(102%) contrast(98%);
        margin-right: 10px;
      }
    </style>
  </head>
  <body>
    <ul class="list-group list-group-flush" id="items">
        <li class="list-group-item m-4 fw-bold d-flex justify-content-between align-items-start" >
            <div>Users</div>
            <button type="button" class="btn btn-danger" data-toggle="button" id="addUser" aria-pressed="false" autocomplete="off">
              Add New User
            </button>
            <span>Action</span>
        </li>
    </ul>

    <nav aria-label="..." style="margin-top: 80px;">
        <ul class="pagination justify-content-center" id="pagination-wrapper">
          <li class="page-item" id="prev">
            <a class="page-link" href="#">Previous</a>
          </li>
          <li class="page-item active" id="1"><a class="page-link" href="#">1</a></li>
          <li class="page-item" id="2">
            <a class="page-link" href="#">2</a>
          </li>
          <li class="page-item" id="3"><a class="page-link" href="#">3</a></li>
          <li class="page-item" id="next">
            <a class="page-link" href="#">Next</a>
          </li>
        </ul>
    </nav>

    <!-- Modal -->
    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-fullscreen">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="exampleModalLabel">User Details</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>

            <div class="modal-body" id="modal-body">
                <ul class="list-group list-group-flush" id="mod-items">
                  <li class="list-group-item m-4 fw-bold d-flex justify-content-between align-items-start" >
                      <div>Projects</div>
                      <button type="button" class="btn btn-secondary" data-toggle="button" id="addProject" aria-pressed="false" autocomplete="off">
                        Add New Project
                      </button>
                      <span>Action</span>
                  </li>
                </ul>
            </div>
        </div>
        </div>
    </div>

    <!-- Project Modal -->
    <div class="modal fade" id="ProjectModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog">
      <div class="modal-content">
          <div class="modal-header">
              <h1 class="modal-title fs-5" id="modal-project-title">Project Description</h1>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>

          <div class="modal-body" id="modal-project">
            Hi!!! I am default Project 

          </div>
      </div>
      </div>
    </div>

    <!-- # addUser-Modal -->
    <div class="modal fade" id="addUserModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">User ðŸ‘¤</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
              <!-- <span aria-hidden="true">&times;</span> -->
            </button>
          </div>
          <div class="modal-body">
            <form>
              <div class="form-group">
                <label for="user-name" class="col-form-label">User Name:</label>
                <input type="text" class="form-control" id="user-name">
              </div>
            </form>
          </div>
          <div class="modal-footer">
            
            <button type="submit" class="btn btn-primary" id="new-user" data-bs-dismiss="modal">Submit</button>
          </div>
        </div>
      </div>
    </div>

    <!-- addProject-Modal -->
    <div class="modal fade" id="addProjectModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Project ðŸ§¾</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
              <!-- <span aria-hidden="true">&times;</span> -->
            </button>
          </div>
          <div class="modal-body">
            <form>
              <div class="form-group">
                <label for="project-name" class="col-form-label">Project Name:</label>
                <input type="text" class="form-control" id="project-name">
              </div>
              <div class="form-group">
                <label for="add-text" class="col-form-label">Project Details:</label>
                <textarea class="form-control" id="add-text"></textarea>
              </div>
              <div class="modal-footer">
            
                <button type="submit" class="btn btn-primary" id="new-project" data-bs-dismiss="modal">Submit</button>
              </div>
            </form>
          </div>
          
        </div>
      </div>
    </div>


    <div class="modal fade" id="editProjectModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Project ðŸ§¾</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
              <!-- <span aria-hidden="true">&times;</span> -->
            </button>
          </div>
          <div class="modal-body">
            <form>
              <div class="form-group">
                <label for="edit-project-name" class="col-form-label">Project Name:</label>
                <input type="text" class="form-control" id="edit-project-name">
              </div>
              <div class="form-group">
                <label for="edit-text" class="col-form-label">Project Details:</label>
                <textarea class="form-control" id="edit-text"></textarea>
              </div>
              <div class="modal-footer">
            
                <button type="submit" class="btn btn-primary" id="edit-project" data-bs-dismiss="modal">Send message</button>
              </div>
            </form>
          </div>
          
        </div>
      </div>
    </div>

    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="./script.js"></script>
</body>
</html>